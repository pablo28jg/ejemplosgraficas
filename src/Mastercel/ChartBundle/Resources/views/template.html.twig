{# app/Resources/views/inicio.html.twig #}
<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<title>{% block titulo %}Adj Web{% endblock %}</title>
		<link type="text/css" rel="stylesheet" href="{{ asset('css/template.css') }}">
		<script src="{{ asset('js/jquery-1.11.0.js') }}" type="text/javascript"></script>
		<script src="{{ asset('js/highcharts.js') }}" type="text/javascript"></script>
		<script type="text/javascript">
			
		</script>
		{% block javascript %}
			
		{% endblock %}
	</head>
	<body>
		<div id="menu">
		{% block menu %}
			
		{% endblock %}
		</div>
		<div id="barralateral">
			{% block barralateral %}
			<ul>
				<li><a href="javascript: addItemBarraEstado('avances','avances','http://localhost/adj/web/app_dev.php/avance','http://localhost/adj/web/app_dev.php/atributo/1/listatributos')">Avances</a></li>
				<li><a href="javascript: addItemBarraEstado('expedientes','expedientes','http://localhost/adj/web/app_dev.php/expediente','http://localhost/adj/web/app_dev.php/atributo/1/listatributos')">Expedientes</a></li>
			</ul>
			{% endblock %}
		</div>
		<div id="contenido">
			{% block cuerpo %}{% endblock %}
		</div>
		<div id="barraestado">
			<div>Ventanas abiertas</div>
			{% block barraestado %} {% endblock %}
		</div>
		<script type="text/javascript">
			{% block javascriptbajo %}{% endblock %}
		</script>
	</body>	
</html>