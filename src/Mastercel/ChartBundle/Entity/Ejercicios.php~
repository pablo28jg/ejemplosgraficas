    private $numtipoestadoperiodo5 = '0';

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="DteInicioPeriodo_6", type="datetime", nullable=true)
     */
    private $dteinicioperiodo6;

    /**
     * @var \DateTime
     *
